{
  "name": "CSU GPU station helper",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "An extension that logins into CSU GPU station and polls the available GPUs. God bless you!",
  "browser_action": {
    "default_icon": "icon16.png",
    "default_menu": "Menu",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "contextMenus",
    "tabs",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  }
}
